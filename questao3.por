programa {
  funcao inicio() {
    //declaração das variaveis 
    inteiro a, b

    //pedir os dados 
    escreva("Olá! Digite o valor 1: ")
    leia(a)
    
    escreva("Digite, agora, o valor 2: ")
    leia(b)

    //condição
    se(a==b){
      inteiro c = a+b
      escreva("O valor é :",c)
    }senao{
      inteiro c = a*b
      escreva("O valor é: ",c)
    }

    



    


  }
}
